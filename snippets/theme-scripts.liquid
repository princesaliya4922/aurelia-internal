{% if template.name == 'product' %}
  {% render 'lazy-js', filename: 'jquery.min.js', loading: 'defer', device: 'desktop' %}
  {% render 'lazy-js', filename: 'slick.min.js', loading: 'defer', device: 'desktop' %}
{% else %}
  <script src="{{ 'jquery.min.js' | asset_url }}"></script>
  <script type="text/javascript" src="{{ 'slick.min.js' | asset_url }}" defer></script>
{% endif %}

{% if template.name == 'product' %}
  {% render 'product-render-js-asset' %}
{% endif %}

<script src="{{ 'vendor-v4.js' | asset_url | split: '?' | first }}" defer></script>
<script src="{{ 'pubsub.js' | asset_url }}" defer></script>
<script src="{{ 'global.js' | asset_url }}" defer></script>

{% if template.name != 'product' %}
  <script src="{{ 'custom.min.js' | asset_url }}" defer></script>
{% else %}
  {% render 'lazy-js', filename: 'custom.min.js', loading: 'lazy', device: 'desktop' %}
{% endif %}

{% if template == 'product' %}
  {% render 'lazy-js', filename: 'pincode-js.js', loading: 'lazy' %}
{% endif %}
